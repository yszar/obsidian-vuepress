{
  "name": "obsidian-vuepress",
  "version": "0.0.2",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "commit": "git cz",
    "release": "dotenv release-it",
    "release-beta": "standard-version -p release --skip.changelog false --skip.tag false",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "release:major": "standard-version -r major -n",
    "release:minor": "standard-version -r minor -n",
    "release:patch": "standard-version -r patch -n",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "obsidian",
    "vuepress"
  ],
  "author": "Jiu Yang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yszar/obsidian-vuepress.git"
  },
  "bugs": {
    "url": "https://github.com/yszar/obsidian-vuepress/issues"
  },
  "homepage": "https://github.com/yszar/obsidian-vuepress#readme",
  "description": "vuepress 构建过程中，将 obsidian 笔记格式替换为 vuepress 兼容的格式",
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "dotenv-cli": "^6.0.0",
    "husky": "^8.0.1",
    "release-it": "^15.5.0"
  }
}
